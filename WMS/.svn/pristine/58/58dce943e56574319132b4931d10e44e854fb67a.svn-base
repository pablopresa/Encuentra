package beans.encuentra;

import java.util.List;

public class RecepcionExpedicion {
	private int idEnvio,unidades;
	private String fecha, choferAc;
	private List <DocumentoEnvio> documentos;	
	
	public int getIdEnvio() {
		return idEnvio;
	}
	public void setIdEnvio(int idEnvio) {
		this.idEnvio = idEnvio;
	}
	public int getUnidades() {
		return unidades;
	}
	public void setUnidades(int unidades) {
		this.unidades = unidades;
	}
	public String getFecha() {
		return fecha;
	}
	public void setFecha(String fecha) {
		this.fecha = fecha;
	}
	public String getChoferAc() {
		return choferAc;
	}
	public void setChoferAc(String choferAc) {
		this.choferAc = choferAc;
	}
	
	
	
	public List<DocumentoEnvio> getDocumentos() {
		return documentos;
	}
	public void setDocumentos(List<DocumentoEnvio> documentos) {
		this.documentos = documentos;
	}
	public RecepcionExpedicion() {
	}
	public RecepcionExpedicion(int idEnvio, int unidades, String fecha, String choferAc) {
		this.idEnvio = idEnvio;
		this.unidades = unidades;
		this.fecha = fecha;
		this.choferAc = choferAc;
	}
}
