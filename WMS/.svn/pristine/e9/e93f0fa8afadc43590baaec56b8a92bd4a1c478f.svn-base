package beans.reportes;

public class PedidosRetrasados {
	private String idPedido, descripcion, sincHoras, estado, destino;

	public PedidosRetrasados(String idPedido, String descripcion, String sincHoras, String estado, String destino) {
		this.idPedido = idPedido;
		this.descripcion = descripcion;
		this.sincHoras = sincHoras;
		this.estado = estado;
		this.destino = destino;
	}

	public String getIdPedido() {
		return idPedido;
	}

	public String getDescripcion() {
		return descripcion;
	}

	public String getSincHoras() {
		return sincHoras;
	}

	public String getEstado() {
		return estado;
	}

	public String getDestino() {
		return destino;
	}
	
}
