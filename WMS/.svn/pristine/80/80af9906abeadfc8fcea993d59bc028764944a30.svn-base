package dataTypes;

public class DataDetallePedidoPrintArt 
{

	private String idArticulo,barra,descripcion;
	private int cantidad;
	private double precioImp;
	public String getIdArticulo() {
		return idArticulo;
	}
	public void setIdArticulo(String idArticulo) {
		this.idArticulo = idArticulo;
	}
	public String getBarra() {
		return barra;
	}
	public void setBarra(String barra) {
		this.barra = barra;
	}
	public int getCantidad() {
		return cantidad;
	}
	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}
	public double getPrecioImp() {
		return precioImp;
	}
	public void setPrecioImp(double precioImp) {
		this.precioImp = precioImp;
	}
	public DataDetallePedidoPrintArt(String idArticulo, String barra, int cantidad, double precioImp, String descripcion) {
		this.idArticulo = idArticulo;
		this.barra = barra;
		this.cantidad = cantidad;
		this.precioImp = precioImp;
		this.descripcion = descripcion;
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	
	
	
	
	
	
	
	
	
	
}
