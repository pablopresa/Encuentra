package beans;

import java.util.Hashtable;
import java.util.List;

import dataTypes.DataIDDescripcion;

public class ArticuloMatrizReposicion 
{
	private String articulo;
	private List <DepositoDimension> depositos;
	Hashtable<Integer, DepositoDimension>depositosHT;
	private List <DataIDDescripcion> talles;
	
	public String getArticulo() {
		return articulo;
	}
	public void setArticulo(String articulo) {
		this.articulo = articulo;
	}
	public List<DepositoDimension> getDepositos() {
		return depositos;
	}
	public void setDepositos(List<DepositoDimension> depositos) {
		this.depositos = depositos;
	}
	public List<DataIDDescripcion> getTalles() {
		return talles;
	}
	public void setTalles(List<DataIDDescripcion> talles) {
		this.talles = talles;
	}
	public ArticuloMatrizReposicion() {
	}
	public Hashtable<Integer, DepositoDimension> getDepositosHT() {
		return depositosHT;
	}
	public void setDepositosHT(Hashtable<Integer, DepositoDimension> depositosHT) {
		this.depositosHT = depositosHT;
	}
	
	

}
