package dataTypes;

import java.util.List;


public class DataGraficoMes 
{
	private int id;
	private String descripcion;
	private List <DataInformeGrafico> lista;
	private int cantidadDama;
	private int cantidadHombre;
	private int cantidadNinio;
	private int cantidadAccesorios;
	
	
	
	public int getCantidadDama() {
		return cantidadDama;
	}
	public void setCantidadDama(int cantidadDama) {
		this.cantidadDama = cantidadDama;
	}
	public int getCantidadHombre() {
		return cantidadHombre;
	}
	public void setCantidadHombre(int cantidadHombre) {
		this.cantidadHombre = cantidadHombre;
	}
	public int getCantidadNinio() {
		return cantidadNinio;
	}
	public void setCantidadNinio(int cantidadNinio) {
		this.cantidadNinio = cantidadNinio;
	}
	public int getCantidadAccesorios() {
		return cantidadAccesorios;
	}
	public void setCantidadAccesorios(int cantidadAccesorios) {
		this.cantidadAccesorios = cantidadAccesorios;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	public List <DataInformeGrafico> getLista() {
		return lista;
	}
	public void setLista(List<DataInformeGrafico> datas) {
		this.lista = datas;
	}
	public DataGraficoMes() {
	}
	
	public String darDescripcionMes(int idMes)
	{
		String mes = "";
		switch (idMes) 
		{
		case 1:
			mes = "EN";
			break;
		case 2:
			mes = "FE";
			break;
		case 3:
			mes = "MA";
			break;
		case 4:
			mes = "AB";
			break;
		case 5:
			mes = "MY";
			break;
		case 6:
			mes = "JN";
			break;
		case 7:
			mes = "JL";
			break;
		case 8:
			mes = "AG";
			break;
		case 9:
			mes = "SE";
			break;
		case 10:
			mes = "OC";
			break;
		case 11:
			mes = "NO";
			break;
		case 12:
			mes = "DI";
			break;
		}
		return mes;	
	}
	
	

}
