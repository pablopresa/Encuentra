package beans.encuentra;

import java.util.List;

public class InfoExpedicion 
{
	private int idEnvio, sentido;
	private String chofer, transporte, fecha;
	private List<InfoDestinoExpedicion> depositos;
	
	
	
	public int getIdEnvio() {
		return idEnvio;
	}



	public void setIdEnvio(int idEnvio) {
		this.idEnvio = idEnvio;
	}



	public int getSentido() {
		return sentido;
	}



	public void setSentido(int sentido) {
		this.sentido = sentido;
	}



	public String getChofer() {
		return chofer;
	}



	public void setChofer(String chofer) {
		this.chofer = chofer;
	}



	public String getTransporte() {
		return transporte;
	}



	public void setTransporte(String transporte) {
		this.transporte = transporte;
	}



	public String getFecha() {
		return fecha;
	}



	public void setFecha(String fecha) {
		this.fecha = fecha;
	}



	public List<InfoDestinoExpedicion> getDepositos() {
		return depositos;
	}



	public void setDepositos(List<InfoDestinoExpedicion> depositos) {
		this.depositos = depositos;
	}



	public InfoExpedicion(int env, String f, String tr, String ch) {
		
		this.idEnvio=env;
		this.fecha=f;
		this.transporte=tr;
		this.chofer=ch;
		// TODO Auto-generated constructor stub
	}
	
	public InfoExpedicion() {
		
		
	}
	
	

}
