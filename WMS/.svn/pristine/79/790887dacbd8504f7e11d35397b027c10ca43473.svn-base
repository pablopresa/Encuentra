package beans.encuentra;

import java.util.List;

import dataTypes.DataIDDescripcion;

public class Envio 
{
	private int idEnvio, sentido;
	private DataIDDescripcion chofer, acompaniante, estado;
	private List<DepositoEnvio> depositos;
	private Transporte transporte;
	private String fecha;
	private String fechaVis;
	private boolean parcial;
	private int remito;
	private String obs;
	
	
	
	
	public int getRemito() {
		return remito;
	}

	public void setRemito(int remito) {
		this.remito = remito;
	}

	public String getObs() {
		return obs;
	}

	public void setObs(String obs) {
		this.obs = obs;
	}

	public boolean isParcial() 
	{
		return parcial;
	}
	
	public void setParcial(boolean parcial) 
	{
		this.parcial = parcial;
	}
	public String getFechaVis() 
	{
		return fechaVis;
	}
	public void setFechaVis(String fechaVis) 
	{
		this.fechaVis = fechaVis;
	}
	public String getFecha() 
	{
		return fecha;
	}
	public void setFecha(String fecha) 
	{
		this.fecha = fecha;
	}
	public Transporte getTransporte() {
		return transporte;
	}
	public void setTransporte(Transporte transporte) {
		this.transporte = transporte;
	}
	public int getIdEnvio() {
		return idEnvio;
	}
	public void setIdEnvio(int idEnvio) {
		this.idEnvio = idEnvio;
	}
	public int getSentido() {
		return sentido;
	}
	public void setSentido(int sentido) {
		this.sentido = sentido;
	}
	public DataIDDescripcion getChofer() {
		return chofer;
	}
	public void setChofer(DataIDDescripcion chofer) {
		this.chofer = chofer;
	}
	public DataIDDescripcion getAcompaniante() {
		return acompaniante;
	}
	public void setAcompaniante(DataIDDescripcion acompaniante) {
		this.acompaniante = acompaniante;
	}
	
	public List<DepositoEnvio> getDepositos() {
		return depositos;
	}
	public void setDepositos(List<DepositoEnvio> depositos) {
		this.depositos = depositos;
	}
	public Envio() {
		
	}

	public DataIDDescripcion getEstado() {
		return estado;
	}

	public void setEstado(DataIDDescripcion estado) {
		this.estado = estado;
	}
	
	
	
	

}
