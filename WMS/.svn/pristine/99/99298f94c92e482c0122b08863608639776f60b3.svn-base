package dataTypes;

public class DataArticuloEcommercePedido 
{
	private int deposito, cantidad, subE;
	private String articulo, comentario, imagen;
	private String fechaPedido,fechaConfirmado;
	private Long idPedido;
	
	
	
	public String getFechaConfirmado() {
		return fechaConfirmado;
	}
	public void setFechaConfirmado(String fechaConfirmado) {
		this.fechaConfirmado = fechaConfirmado;
	}
	public String getFechaPedido() {
		return fechaPedido;
	}
	public void setFechaPedido(String fechaPedido) {
		this.fechaPedido = fechaPedido;
	}
	public Long getIdPedido() {
		return idPedido;
	}
	public void setIdPedido(Long idPedido) {
		this.idPedido = idPedido;
	}
	public int getDeposito() {
		return deposito;
	}
	public void setDeposito(int deposito) {
		this.deposito = deposito;
	}
	public int getCantidad() {
		return cantidad;
	}
	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}
	public String getArticulo() {
		return articulo;
	}
	public void setArticulo(String articulo) {
		this.articulo = articulo;
	}
	public String getComentario() {
		return comentario;
	}
	public void setComentario(String comentario) {
		this.comentario = comentario;
	}	
	
	public int getSubE() {
		return subE;
	}
	public void setSubE(int subE) {
		this.subE = subE;
	}
	public DataArticuloEcommercePedido(Long idPedido, int deposito,int cantidad, String articulo, String fechaPedido, int sub) 
	{
		this.idPedido = idPedido;
		this.deposito = deposito;
		this.cantidad = cantidad;
		this.articulo = articulo;
		this.fechaPedido = fechaPedido;
		this.subE = sub;
	}
	public String getImagen() {
		return articulo.substring(0,13);
	}
	public void setImagen(String imagen) {
		this.imagen = imagen;
	}
	
	
	
	
	
	
	

}
