package beans;

import java.util.List;

import dataTypes.DataIDDescDescripcion;

public class UsuarioLDAP {

	private String nombre;
	private String mail;
	private String cn;
	private List<DataIDDescDescripcion> gruposIn;
	private boolean esAdmin;
	private int id;
	
	
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public String getMail() {
		return mail;
	}
	public void setMail(String mail) {
		this.mail = mail;
	}
	public List<DataIDDescDescripcion> getGruposIn() {
		return gruposIn;
	}
	public void setGruposIn(List<DataIDDescDescripcion> gruposIn) {
		this.gruposIn = gruposIn;
	}
	public UsuarioLDAP(String nombre, String mail,List<DataIDDescDescripcion> gruposIn, boolean admin) 
	{
		this.nombre = nombre;
		this.mail = mail;
		this.gruposIn = gruposIn;
		this.esAdmin = admin;
	}
	public boolean isEsAdmin() {
		return esAdmin;
	}
	public void setEsAdmin(boolean esAdmin) {
		this.esAdmin = esAdmin;
	}
	public UsuarioLDAP() {
	}
	public String getCn() {
		return cn;
	}
	public void setCn(String cn) {
		this.cn = cn;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	
	
	
	
	
	
	

}
