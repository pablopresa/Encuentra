package dataTypes;

import java.util.List;

public class DataLista 
{
	private int total;
	private double totalDouble;
	private int cantidad;
	private List <DataCostoReparacion> lista;
	
	
	public int getTotal() {
		return total;
	}
	public void setTotal(int total) {
		this.total = total;
	}
	
	public double getTotalDouble() {
		return totalDouble;
	}
	public void setTotalDouble(double totalDouble) {
		this.totalDouble = totalDouble;
	}
	public int getCantidad() {
		return cantidad;
	}
	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}

	
	public DataLista() 
	{
	}
	public List<DataCostoReparacion> getLista() {
		return lista;
	}
	public void setLista(List<DataCostoReparacion> lista) {
		this.lista = lista;
	}
	
	
	
	

}
