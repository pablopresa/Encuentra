package beans.encuentra;

import java.util.List;



public class ColorOC 
{
	private String descripcion;
	private String id;
	private int idColorBase;
	private java.util.Hashtable<String, TalleOC> tallesH;
	private List<TalleOC> talles;
	private int etiqueta;
	private int distribuir;
	private boolean mostrar;
	
	
		
	
	public boolean isMostrar() {
		return mostrar;
	}
	public void setMostrar(boolean mostrar) {
		this.mostrar = mostrar;
	}
	public int getEtiqueta() {
		return etiqueta;
	}
	public void setEtiqueta(int etiqueta) {
		this.etiqueta = etiqueta;
	}
	public int getDistribuir() {
		return distribuir;
	}
	public void setDistribuir(int distribuir) {
		this.distribuir = distribuir;
	}
	public java.util.Hashtable<String, TalleOC> getTallesH() {
		return tallesH;
	}
	public void setTallesH(java.util.Hashtable<String, TalleOC> tallesH) {
		this.tallesH = tallesH;
	}
	public List<TalleOC> getTalles() {
		return talles;
	}
	public void setTalles(List<TalleOC> talles) {
		this.talles = talles;
	}
	public ColorOC() {
		super();
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public int getIdColorBase() {
		return idColorBase;
	}
	public void setIdColorBase(int idColorBase) {
		this.idColorBase = idColorBase;
	}

	
	

}
