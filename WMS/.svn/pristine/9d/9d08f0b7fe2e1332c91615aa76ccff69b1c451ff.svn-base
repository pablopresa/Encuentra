package dataTypes;

public class DataEventoArreglo 
{
	private int sucursal;
	private String fecha;
	private String articulo;
	private String descEstado;
	private String descResolucion;
	private String nombreFuncionario;
	private int idTrazabilidad;
	private boolean borrado;
	private int numFunc;
	
	
	
	
	public int getIdTrazabilidad() {
		return idTrazabilidad;
	}
	public void setIdTrazabilidad(int idTrazabilidad) {
		this.idTrazabilidad = idTrazabilidad;
	}
	public int getSucursal() {
		return sucursal;
	}
	public void setSucursal(int sucursal) {
		this.sucursal = sucursal;
	}
	public String getFecha() {
		return fecha;
	}
	public void setFecha(String fecha) {
		this.fecha = fecha;
	}
	public String getArticulo() {
		return articulo;
	}
	public void setArticulo(String articulo) {
		this.articulo = articulo;
	}
	public String getDescEstado() {
		return descEstado;
	}
	public void setDescEstado(String descEstado) {
		this.descEstado = descEstado;
	}
	public String getDescResolucion() {
		return descResolucion;
	}
	public void setDescResolucion(String descResolucion) {
		this.descResolucion = descResolucion;
	}
	public String getNombreFuncionario() {
		return nombreFuncionario;
	}
	public void setNombreFuncionario(String nombreFuncionario) {
		this.nombreFuncionario = nombreFuncionario;
	}
	public DataEventoArreglo(int sucursal, String fecha, String articulo,
			String descEstado, String descResolucion, String nombreFuncionario) {
		this.sucursal = sucursal;
		this.fecha = fecha;
		this.articulo = articulo;
		this.descEstado = descEstado;
		this.descResolucion = descResolucion;
		this.nombreFuncionario = nombreFuncionario;
	}
	public DataEventoArreglo() {
	}
	public void setBorrado(boolean borrado) {
		this.borrado = borrado;
	}
	public boolean isBorrado() {
		return borrado;
	}
	public void setNumFunc(int numFunc) {
		this.numFunc = numFunc;
	}
	public int getNumFunc() {
		return numFunc;
	}
	
	
	

}
