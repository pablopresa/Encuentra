package beans.reportes;

public class ConteoOjo 
{
	private String estanteria,idOjo,idArticulo,fecha_conteo,fecha_Act,usuario,descripcionArticulo;
	private int modulo,estante,cantidad_ojo,cantidad_contada,diferencia;
	public String getEstanteria() {
		return estanteria;
	}
	
	
	
	public String getDescripcionArticulo() {
		return descripcionArticulo;
	}



	public void setDescripcionArticulo(String descripcionArticulo) {
		this.descripcionArticulo = descripcionArticulo;
	}



	public int getDiferencia() {
		int dif = this.cantidad_ojo-this.getCantidad_contada();
		if(dif<0)
		{
			dif = dif*-1;
		}
			
		return dif;
	}
	public void setEstanteria(String estanteria) {
		this.estanteria = estanteria;
	}
	public String getIdOjo() {
		return idOjo;
	}
	public void setIdOjo(String idOjo) {
		this.idOjo = idOjo;
	}
	public String getIdArticulo() {
		return idArticulo;
	}
	public void setIdArticulo(String idArticulo) {
		this.idArticulo = idArticulo;
	}
	public String getFecha_conteo() {
		return fecha_conteo;
	}
	public void setFecha_conteo(String fecha_conteo) {
		this.fecha_conteo = fecha_conteo;
	}
	public String getFecha_Act() {
		return fecha_Act;
	}
	public void setFecha_Act(String fecha_Act) {
		this.fecha_Act = fecha_Act;
	}
	public String getUsuario() {
		return usuario;
	}
	public void setUsuario(String usuario) {
		this.usuario = usuario;
	}
	public int getModulo() {
		return modulo;
	}
	public void setModulo(int modulo) {
		this.modulo = modulo;
	}
	public int getEstante() {
		return estante;
	}
	public void setEstante(int estante) {
		this.estante = estante;
	}
	public int getCantidad_ojo() {
		return cantidad_ojo;
	}
	public void setCantidad_ojo(int cantidad_ojo) {
		this.cantidad_ojo = cantidad_ojo;
	}
	public int getCantidad_contada() {
		return cantidad_contada;
	}
	public void setCantidad_contada(int cantidad_contada) {
		this.cantidad_contada = cantidad_contada;
	}
	public ConteoOjo(String estanteria,int modulo,int estante,String idOjo,String idArticulo,int cantidad_ojo,int cantidad_contada,String fecha_conteo,String fecha_Act, String usuario, String descripcionArticulo) 
	{
		this.estanteria = estanteria;
		this.idOjo = idOjo;
		this.idArticulo = idArticulo;
		this.fecha_conteo = fecha_conteo;
		this.fecha_Act = fecha_Act;
		this.usuario = usuario;
		this.modulo = modulo;
		this.estante = estante;
		this.cantidad_ojo = cantidad_ojo;
		this.cantidad_contada = cantidad_contada;
		this.descripcionArticulo = descripcionArticulo;
	}

	

}
