package beans.reportes;

public class MonitorVentaMArticulo {
	private String fecha,articulo,estado,justificacion;
	private int pedido,cantidad, picked,verificadas,remitidas,nroCaja, idPicking;
	

	public MonitorVentaMArticulo(String fecha, String articulo,String justificacion, int pedido,int cantidad, int picked, int verificadas, int remitidas, int nroCaja, String estado, int idPick) 
	{
		this.fecha = fecha;
		this.articulo = articulo;
		this.cantidad = cantidad;
		this.picked = picked;
		this.verificadas = verificadas;
		this.remitidas = remitidas;
		this.nroCaja = nroCaja;
		this.estado = estado;
		this.pedido = pedido;
		this.justificacion = justificacion;
		this.setIdPicking(idPick);
	}


	public String getFecha() {
		return fecha;
	}


	public void setFecha(String fecha) {
		this.fecha = fecha;
	}


	public String getArticulo() {
		return articulo;
	}


	public void setArticulo(String articulo) {
		this.articulo = articulo;
	}


	public String getEstado() {
		return estado;
	}


	public void setEstado(String estado) {
		this.estado = estado;
	}


	public String getJustificacion() {
		return justificacion;
	}


	public void setJustificacion(String justificacion) {
		this.justificacion = justificacion;
	}


	public int getPedido() {
		return pedido;
	}


	public void setPedido(int pedido) {
		this.pedido = pedido;
	}


	public int getCantidad() {
		return cantidad;
	}


	public void setCantidad(int cantidad) {
		this.cantidad = cantidad;
	}


	public int getPicked() {
		return picked;
	}


	public void setPicked(int picked) {
		this.picked = picked;
	}


	public int getVerificadas() {
		return verificadas;
	}


	public void setVerificadas(int verificadas) {
		this.verificadas = verificadas;
	}


	public int getRemitidas() {
		return remitidas;
	}


	public void setRemitidas(int remitidas) {
		this.remitidas = remitidas;
	}


	public int getNroCaja() {
		return nroCaja;
	}


	public void setNroCaja(int nroCaja) {
		this.nroCaja = nroCaja;
	}


	public int getIdPicking() {
		return idPicking;
	}


	public void setIdPicking(int idPicking) {
		this.idPicking = idPicking;
	}

	
}
