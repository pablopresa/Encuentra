package beans.reportes;

import java.util.List;

import dataTypes.DataIDDescripcion;

public class StockEncuentraVisual {
	private String idArticulo, sotckEncuentra, stockVisual;
	private List<DataIDDescripcion> ojosCantidad;
	
	public StockEncuentraVisual(String idArticulo, String sotckEncuentra, String stockVisual,
			List<DataIDDescripcion> ojosCantidad) {
		this.idArticulo = idArticulo;
		this.sotckEncuentra = sotckEncuentra;
		this.stockVisual = stockVisual;
		this.ojosCantidad = ojosCantidad;
	}

	public String getIdArticulo() {
		return idArticulo;
	}

	public String getSotckEncuentra() {
		return sotckEncuentra;
	}

	public String getStockVisual() {
		return stockVisual;
	}

	public List<DataIDDescripcion> getOjosCantidad() {
		return ojosCantidad;
	}

	public void setIdArticulo(String idArticulo) {
		this.idArticulo = idArticulo;
	}

	public void setSotckEncuentra(String sotckEncuentra) {
		this.sotckEncuentra = sotckEncuentra;
	}

	public void setStockVisual(String stockVisual) {
		this.stockVisual = stockVisual;
	}

	public void setOjosCantidad(List<DataIDDescripcion> ojosCantidad) {
		this.ojosCantidad = ojosCantidad;
	}
	
}
