package beans.encuentra;

import dataTypes.DataIDDescripcion;

public class TaskType {
	 int id;
	 String Descripcion;
	 int unidades;
	 int minutos;
	 DataIDDescripcion area;
	 DataIDDescripcion resolucion;
	 
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getDescripcion() {
		return Descripcion;
	}
	public void setDescripcion(String descripcion) {
		Descripcion = descripcion;
	}
	public int getUnidades() {
		return unidades;
	}
	public void setUnidades(int unidades) {
		this.unidades = unidades;
	}
	public int getMinutos() {
		return minutos;
	}
	public void setMinutos(int minutos) {
		this.minutos = minutos;
	}
	public DataIDDescripcion getArea() {
		return area;
	}
	public void setArea(DataIDDescripcion area) {
		this.area = area;
	}
	public DataIDDescripcion getResolucion() {
		return resolucion;
	}
	public void setResolucion(DataIDDescripcion resolucion) {
		this.resolucion = resolucion;
	}
	 

}
