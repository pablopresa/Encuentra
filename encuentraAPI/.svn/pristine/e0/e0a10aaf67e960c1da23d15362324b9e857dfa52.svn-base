package beans.datatypes;

public class MetodoPago {
	private int idMetodoPago;
	private String descripcion;
	private String giftcard_order;
	private Double monto;
	
	
	public int getIdMetodoPago() {
		return idMetodoPago;
	}
	public String getDescripcion() {
		return descripcion;
	}
	
	public void setIdMetodoPago(int idMetodoPago) {
		this.idMetodoPago = idMetodoPago;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	
	public String getGiftcard_order() {
		return giftcard_order;
	}
	public void setGiftcard_order(String giftcard_order) {
		this.giftcard_order = giftcard_order;
	}
	
	public MetodoPago (int idm, String desc, String gift,Double amount) {
		this.idMetodoPago = idm;
		this.descripcion = desc;
		this.giftcard_order = gift;
		this.monto = amount;
	}
	public Double getMonto() {
		return monto;
	}
	public void setMonto(Double monto) {
		this.monto = monto;
	}
	
}
