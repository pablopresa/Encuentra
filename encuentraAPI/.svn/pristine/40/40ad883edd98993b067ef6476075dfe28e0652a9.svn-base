package integraciones.erp.odoo.laIsla;

public class EstadoAlbaranes {
	private int id;
	private String state;
	
	public EstadoAlbaranes(int id, String state) {
		this.id = id;
		this.state = state;
	}
	
	public EstadoAlbaranes(int id) {
		this.id = id;
	}

	public int getId() {
		return id;
	}

	public String getState() {
		return state;
	}

	public void setId(int nroAlbaran) {
		this.id = nroAlbaran;
	}

	public void setState(String state) {
		this.state = state;
	}
	
	@Override
	public String toString() {
		return "{"
				+ "id = " + id + ","
				+ "state = '" + state + "'"
				+ "}";
	}
}
