package clienteLaIsla;

public class Articulo {
	private String name, sexo, temporada, ean13, marca, categoria, default_code, talle_proveedor, color_proveedor, seller_ids;
	
	public Articulo(String name, String sexo, String temporada, String ean13, String marca, String categoria,
			String default_code, String talle_proveedor, String color_proveedor, String seller_ids) {
		this.name = name;
		this.sexo = sexo;
		this.temporada = temporada;
		this.ean13 = ean13;
		this.marca = marca;
		this.categoria = categoria;
		this.default_code = default_code;
		this.talle_proveedor = talle_proveedor;
		this.color_proveedor = color_proveedor;
		this.seller_ids = seller_ids;
	}

	public String getName() {
		return name;
	}

	public String getSexo() {
		return sexo;
	}

	public String getTemporada() {
		return temporada;
	}

	public String getEan13() {
		return ean13;
	}

	public String getMarca() {
		return marca;
	}

	public String getCategoria() {
		return categoria;
	}

	public String getDefault_code() {
		return default_code;
	}

	public String getTalle_proveedor() {
		return talle_proveedor;
	}

	public String getColor_proveedor() {
		return color_proveedor;
	}

	public String getSeller_ids() {
		return seller_ids;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setSexo(String sexo) {
		this.sexo = sexo;
	}

	public void setTemporada(String temporada) {
		this.temporada = temporada;
	}

	public void setEan13(String ean13) {
		this.ean13 = ean13;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}

	public void setCategoria(String categoria) {
		this.categoria = categoria;
	}

	public void setDefault_code(String default_code) {
		this.default_code = default_code;
	}

	public void setTalle_proveedor(String talle_proveedor) {
		this.talle_proveedor = talle_proveedor;
	}

	public void setColor_proveedor(String color_proveedor) {
		this.color_proveedor = color_proveedor;
	}

	public void setSeller_ids(String seller_ids) {
		this.seller_ids = seller_ids;
	}
	
	@Override
	public String toString() {
		return this.name + " - " + this.sexo + " - " + this.temporada + " - " + this.ean13 + " - " + this.marca + " - " + this.categoria
				 + " - " + this.default_code + " - " + this.talle_proveedor + " - " + this.color_proveedor + " - " + this.seller_ids;
	}
}
