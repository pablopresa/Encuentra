package dataTypes;

import java.util.ArrayList;
import java.util.List;

import fenicioAPI.Descuento;

public class BillingEntities {
	private List<MetodoPago> metodos_pagos;
	private List<Descuento> descuentos;
	
	
	public List<MetodoPago> getMetodos_pagos() {
		return metodos_pagos;
	}
	public List<Descuento> getDescuentos() {
		return descuentos;
	}
	public void setMetodos_pagos(List<MetodoPago> metodos_pagos) {
		this.metodos_pagos = metodos_pagos;
	}
	public void setDescuentos(List<Descuento> descuentos) {
		this.descuentos = descuentos;
	}
	
	public BillingEntities() {
		this.metodos_pagos = new ArrayList<MetodoPago>();
		this.descuentos = new ArrayList<Descuento>();
	}
}
