package integraciones.erp.odoo.laIsla;

import beans.encuentra.Cliente;

public class ArticuloCantidadEncuentra {
	private int idDeposito;
	private double cantidad;
	private String idArticulo;
	private Cliente cliente;
	
	public ArticuloCantidadEncuentra(double cantidad, int idDeposito, String idArticulo) {
		this.cantidad = cantidad;
		this.idDeposito = idDeposito;
		this.idArticulo = idArticulo;
	}
	
	
	
	public ArticuloCantidadEncuentra(double cantidad, int idDeposito, String idArticulo, Cliente cliente) {
		this.cantidad = cantidad;
		this.idDeposito = idDeposito;
		this.idArticulo = idArticulo;
		this.cliente = cliente;
	}



	public double getCantidad() {
		return cantidad;
	}
	public int getIdDeposito() {
		return idDeposito;
	}
	public String getIdArticulo() {
		return idArticulo;
	}
	public void setCantidad(double cantidad) {
		this.cantidad = cantidad;
	}
	public void setIdDeposito(int idDeposito) {
		this.idDeposito = idDeposito;
	}
	public void setIdArticulo(String idArticulo) {
		this.idArticulo = idArticulo;
	}



	public Cliente getCliente() {
		return cliente;
	}
 


	public void setCliente(Cliente cliente) {
		this.cliente = cliente;
	}	
	
}
