package integraciones.erp.visualStore.objetos;

public class ParametrosVisual 
{

	private int largoBase,largoColor,largoTalle;
	private String urlImg;
	public int getLargoBase() {
		return largoBase;
	}
	public void setLargoBase(int largoBase) {
		this.largoBase = largoBase;
	}
	public int getLargoColor() {
		return largoColor;
	}
	public void setLargoColor(int largoColor) {
		this.largoColor = largoColor;
	}
	public int getLargoTalle() {
		return largoTalle;
	}
	public void setLargoTalle(int largoTalle) {
		this.largoTalle = largoTalle;
	}
	public String getUrlImg() {
		return urlImg;
	}
	public void setUrlImg(String urlImg) {
		this.urlImg = urlImg;
	}
	public ParametrosVisual(int largoBase, int largoColor, int largoTalle, String urlImg) {
		this.largoBase = largoBase;
		this.largoColor = largoColor;
		this.largoTalle = largoTalle;
		this.urlImg = urlImg;
	}
	
	
	
	
	
}
