package integraciones.marketplaces.fenicio.apiComercios;

public class Variantes
{
    private String codigo;

    private String orden;

    private Presentaciones[] presentaciones;

    private String nombre;

    private Atributos atributos;

    public String getCodigo ()
    {
        return codigo;
    }

    public void setCodigo (String codigo)
    {
        this.codigo = codigo;
    }

    public String getOrden ()
    {
        return orden;
    }

    public void setOrden (String orden)
    {
        this.orden = orden;
    }

    public Presentaciones[] getPresentaciones ()
    {
        return presentaciones;
    }

    public void setPresentaciones (Presentaciones[] presentaciones)
    {
        this.presentaciones = presentaciones;
    }

    public String getNombre ()
    {
        return nombre;
    }

    public void setNombre (String nombre)
    {
        this.nombre = nombre;
    }

    public Atributos getAtributos ()
    {
        return atributos;
    }

    public void setAtributos (Atributos atributos)
    {
        this.atributos = atributos;
    }

    @Override
    public String toString()
    {
        return "ClassPojo [codigo = "+codigo+", orden = "+orden+", presentaciones = "+presentaciones+", nombre = "+nombre+", atributos = "+atributos+"]";
    }
}
			
			