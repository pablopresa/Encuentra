package integraciones.erp.odoo.laIsla;

import java.util.List;

public class PedidoAlbaran {
	private List<ArticuloCantidadEncuentra> articulosCantidadCliente;
	private int destino;
	
	public PedidoAlbaran(List<ArticuloCantidadEncuentra> aec, int destino) {
		this.articulosCantidadCliente = aec;
		this.destino = destino;
	}

	public List<ArticuloCantidadEncuentra> getAec() {
		return articulosCantidadCliente;
	}

	public int getDestino() {
		return destino;
	}

	public void setAec(List<ArticuloCantidadEncuentra> aec) {
		this.articulosCantidadCliente = aec;
	}

	public void setDestino(int destino) {
		this.destino = destino;
	}
	
}
